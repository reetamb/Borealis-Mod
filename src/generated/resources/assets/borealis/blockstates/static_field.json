{
  "multipart": [
    {
      "apply": {
        "model": "borealis:block/static_field_post"
      }
    },
    {
      "apply": {
        "model": "borealis:block/static_field_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "borealis:block/static_field_noside"
      },
      "when": {
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "borealis:block/static_field_side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "borealis:block/static_field_noside_alt",
        "y": 90
      },
      "when": {
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "borealis:block/static_field_side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "borealis:block/static_field_noside",
        "y": 270
      },
      "when": {
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "borealis:block/static_field_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "borealis:block/static_field_noside_alt"
      },
      "when": {
        "east": "false"
      }
    }
  ]
}